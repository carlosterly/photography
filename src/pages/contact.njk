<form name="contact-form" method="post" data-netlify="true" netlify-honeypot="bot-field" action="/thankyou">

  <fieldset class="fieldset">

    <p class="hidden">
      <label>
      Don't fill this out if you're human: <input name="bot-field"/>
      </label>
    </p>

    <label for="full-name">Full Name</label>
    <input class="input" type="text" name="name" id="full-name" required="">
    <label for="email-address">Email Address</label>
    <input class="input" type="email" name="_replyto" id="email-address" required="">

    <label for="timely">Type of enquiry</label>
    <select class="select" name="timely" id="timely" required="">
      <option value="" selected="" disabled="">Choose</option>
      <option value="TFP Photo Shoot">TFP Photo Shoot</option>
      <option value="Paid Photo Shoot">Paid Photo Shoot</option>
      <option value="Other">Other</option>
    </select>

    <label for="message">Additional Comments</label>
    <textarea class="input" rows="3" name="message" id="message" required=""></textarea>
    <input type="hidden" name="subject" value="Photography Enquiry">
  </fieldset>

  <input class="btn btn--primary outline" type="submit" value="Submit">

</form>